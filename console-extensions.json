[
  {
    "type": "console.flag",
    "properties": {
      "handler": {
        "$codeRef": "features.setODFFlag"
      }
    }
  },
  {
    "type": "console.navigation/href",
    "properties": {
      "id": "odfdashboard",
      "insertBefore": "persistentvolumes",
      "section": "storage",
      "name": "Data Foundation",
      "href": "/odf/cluster"
    },
    "flags": {
      "required": ["ODF_MODEL"],
      "disallowed": ["ODF_MANAGED"]
    }
  },
  {
    "type": "console.page/route",
    "properties": {
      "path": ["/odf/cluster", "/odf/systems"],
      "exact": false,
      "component": { "$codeRef": "dashboard.default" }
    },
    "flags": {
      "required": ["ODF_MODEL"],
      "disallowed": ["ODF_MANAGED"]
    }
  },
  {
    "type": "console.page/route",
    "properties": {
      "path": ["/odf", "/odf/overview"],
      "exact": true,
      "component": { "$codeRef": "dashboard.Reroute" }
    },
    "flags": {
      "required": ["ODF_MODEL"]
    }
  },
  {
    "type": "console.page/route",
    "properties": {
      "path": "/odf/resource/:kind",
      "exact": true,
      "component": { "$codeRef": "dashboard.RerouteResource" }
    },
    "flags": {
      "required": ["ODF_MODEL"]
    }
  },
  {
    "type": "console.flag/model",
    "properties": {
      "model": {
        "group": "ramendr.openshift.io",
        "version": "v1alpha1",
        "kind": "DRPolicy"
      },
      "flag": "ODR_MODEL"
    }
  },
  {
    "type": "console.navigation/section",
    "properties": {
      "id": "data-service-section",
      "perspective": "acm",
      "name": "Data Services"
    },
    "flags": {
      "required": ["ODR_MODEL"]
    }
  },
  {
    "type": "console.navigation/href",
    "properties": {
      "id": "datapolicy",
      "perspective": "acm",
      "section": "data-service-section",
      "name": "Data Policy",
      "href": "/multicloud/dataservices/datapolicy"
    },
    "flags": {
      "required": ["ODR_MODEL"]
    }
  },
  {
    "type": "console.page/route",
    "properties": {
      "path": ["/multicloud/dataservices/datapolicy"],
      "exact": false,
      "component": { "$codeRef": "datapolicy.default" }
    },
    "flags": {
      "required": ["ODR_MODEL"]
    }
  }
]
